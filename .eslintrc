{
  "extends": ["oclif", "oclif-typescript"],
  "rules": {
    "arrow-parens": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "lines-between-class-members": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "unicorn/no-array-for-each": "off"
  }
}
